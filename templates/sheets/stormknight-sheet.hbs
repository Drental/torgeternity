<form class="actor-sheet {{cssClass}}" autocomplete="off">
    <header class="sheet-header">
        <img src="{{actor.img}}" class="profile-img" data-edit="img" title="{{actor.name}}"  />
        <h1 class="charname"><input name="name" type="text" value="{{actor.name}}" placeholder="Name"
                style="font-family:'Palatino Linotype';background-color:white;border-radius:10px" /></h1>
        <img src="systems/torgeternity/images/te-logo.png" class="profile-img"
            style="border:none;padding-left:5px;padding-right:5px;height:40px" title="TE Logo" />
    </header>
    <div>
        <table style="border:none;background:none;font-family:'Palatino Linotype';font-size:14;font-weight:bold">
            <tr>
                <td style="text-align:center; font-size:11px" width="20%">
                    <span class="vital-box">
                        {{localize "torgeternity.sheetLabels.wounds"}}: 
                        <input name="data.wounds.value" type="number" value="{{data.wounds.value}}" data-dtype="Number" class="small-input" />
                        /
                        <span class="storm-knight-label-small">{{data.wounds.max}}</span>
                    </span>
                </td>
                    <td style="text-align:center; font-size:11px" width="20%">
                        <span class="vital-box">
                            {{localize "torgeternity.sheetLabels.shock"}}: 
                            <input name="data.shock.value" type="number" value="{{data.shock.value}}" data-dtype="Number" class="small-input" />
                            /
                            <span class="storm-knight-label-small">{{data.shock.max}}</span>
                        </span>
                    </td>
                <td style="text-align:center; font-size:11px" width="20%">
                    <span class="vital-box">
                        {{localize "torgeternity.sheetLabels.possibilities"}}:
                        <input name="data.other.possibilities" type="number" value="{{data.other.possibilities}}" data-dtype="Number" class="small-input" />
                    </span>
                </td>
            </tr>
        </table>
    </div>


    {{!-- Sheet Tab Navigation --}}
    <nav class="sheet-tabs tabs" data-group="primary"
        style="background-image:url('systems/torgeternity/images/tab-header.png');background-size:578px 30px;background-repeat:no-repeat;color:white">
        <a class="item" data-tab="stats" style="vertical-align:center">{{localize "torgeternity.sheetLabels.stats"}}</a>
        <a class="item" data-tab="perks">{{localize "torgeternity.sheetLabels.perks"}}</a>
        <a class="item" data-tab="gear">{{localize "torgeternity.sheetLabels.gear"}}</a>
        <a class="item" data-tab="powers">{{localize "torgeternity.sheetLabels.powers"}}</a>
        <a class="item" data-tab="effects">{{localize "torgeternity.sheetLabels.effects"}}</a>
        <a class="item" data-tab="background">{{localize "torgeternity.sheetLabels.background"}}</a>
    </nav>

    <section class="sheet-body" style="display:flex;flex-direction:column">

        <div class="tab stats" data-group="primary" data-tab="stats">
            <table style="border:none;background-color:transparent;margin-top:0px;margin-bottom:0px">
                <td width="20%" style="margin:3px;vertical-align:top">
                    <div class="list-section-heading">
                        {{localize "torgeternity.sheetLabels.attributes"}}
                    </div>
                    <table class="stats-table">
                        <tr>
                            <td>
                                <a class="skill-roll" data-testType="attribute"
                                    data-baseAttribute="{{data.attributes.charisma}}" data-adds="0"
                                    data-value="{{data.attributes.charisma}}" data-unskilledUse="1"
                                    data-name="{{localize " torgeternity.attributes.charisma"}}">{{localize
                                    "torgeternity.attributes.charisma"}}</a>
                            </td>
                            <td>
                                <input name="data.attributes.charisma" type="number" value="{{data.attributes.charisma}}" data=dtype="Number" class="small-input" /> 
                            </td>
                        </tr>
                        <tr>
                            <td>
                                <a class="skill-roll" data-testType="attribute"
                                    data-baseAttribute="{{data.attributes.dexterity}}" data-adds="0"
                                    data-value="{{data.attributes.dexterity}}" data-unskilledUse="1"
                                    data-name="{{localize " torgeternity.attributes.dexterity"}}">{{localize
                                    "torgeternity.attributes.dexterity"}}</a>
                            </td>
                            <td>
                                <input name="data.attributes.dexterity" type="number" value="{{data.attributes.dexterity}}" data=dtype="Number" class="small-input" />
                            </td>
                        </tr>
                        <tr>
                            <td>
                                <a class="skill-roll" data-testType="attribute"
                                    data-baseAttribute="{{data.attributes.mind}}" data-adds="0"
                                    data-value="{{data.attributes.mind}}" data-unskilledUse="1" data-name="{{localize "
                                    torgeternity.attributes.mind"}}">{{localize "torgeternity.attributes.mind"}}</a>
                            </td>
                            <td>
                                <input name="data.attributes.mind" type="number" value="{{data.attributes.mind}}" data=dtype="Number" class="small-input" />
                            </td>
                        </tr>
                        <tr>
                            <td>
                                <a class="skill-roll" data-testType="attribute"
                                    data-baseAttribute="{{data.attributes.spirit}}" data-adds="0"
                                    data-value="{{data.attributes.spirit}}" data-unskilledUse="1"
                                    data-name="{{localize " torgeternity.attributes.spirit"}}">{{localize
                                    "torgeternity.attributes.spirit"}}</a>
                            </td>
                            <td>
                                <input name="data.attributes.spirit" type="number" value="{{data.attributes.spirit}}" data=dtype="Number" class="small-input" />
                            </td>
                        </tr>
                        <tr>
                            <td>
                                <a class="skill-roll" data-testType="attribute"
                                    data-baseAttribute="{{data.attributes.strength}}" data-adds="0"
                                    data-value="{{data.attributes.strength}}" data-unskilledUse="1"
                                    data-name="{{localize " torgeternity.attributes.strength"}}">{{localize
                                    "torgeternity.attributes.strength"}}</a>
                            </td>
                            <td>
                                <input name="data.attributes.strength" type="number" value="{{data.attributes.strength}}" data=dtype="Number" class="small-input" />
                            </td>
                        </tr>
                    </table>

                    <table class="stats-table">
                        <tr>
                            <td>
                                <span class="main-label">{{localize "torgeternity.sheetLabels.move"}}</span>
                            </td>
                            <td>
                                <span class="storm-knight-label">{{data.other.move}}</span>
                            </td>
                        </tr>
                        <tr>
                            <td>
                                <span class="main-label">{{localize "torgeternity.sheetLabels.run"}}</span>
                            </td>
                            <td>
                                <span class="storm-knight-label">{{data.other.run}}</span>
                            </td>
                        </tr>
                        <tr>
                            <td>
                                <span class="main-label">{{localize "torgeternity.sheetLabels.toughness"}}</span>
                            </td>
                            <td>
                                <span class="storm-knight-label">{{data.other.toughness}}</span>
                            </td>
                        </tr>
                        <tr>
                            <td>
                                <span class="main-label">{{localize "torgeternity.sheetLabels.armor"}}</span>
                            </td>
                            <td>
                                <input name="data.other.armor" type="number" value="{{data.other.armor}}" data=dtype="Number" class="small-input" />
                            </td>
                        </tr>
                    </table>


                    <div class="list-section-heading">
                        Cosm
                    </div>
                    <select name="data.other.cosm" class="input-box" style="width:100%;font-size:14px">
                        {{#select data.other.cosm}}
                        {{#each config.cosmTypes as |name type|}}
                        <option value="{{type}}">{{localize name}}</option>
                        {{/each}}
                        {{/select}}
                    </select>
                    <table class="stats-table">
                        <tr>
                            <td>
                                <span class="main-label">{{localize "torgeternity.sheetLabels.magic"}}</span>
                            </td>
                            <td>
                                <span class="storm-knight-label">{{data.axioms.magic}}</span>
                        </tr>
                        <tr>
                            <td>
                                <span class="main-label">{{localize "torgeternity.sheetLabels.social"}}</span>
                            </td>
                            <td>
                                <span class="storm-knight-label">{{data.axioms.social}}</span>
                            </td>
                        </tr>
                        <tr>
                            <td>
                                <span class="main-label">{{localize "torgeternity.sheetLabels.spirit"}}</span>
                            </td>
                            <td>
                                <span class="storm-knight-label">{{data.axioms.spirit}}</span>
                            </td>
                        </tr>
                        <tr>
                            <td>
                                <span class="main-label">{{localize "torgeternity.sheetLabels.tech"}}</span>
                            </td>
                            <td>
                                <span class="storm-knight-label">{{data.axioms.tech}}</span>
                            </td>
                        </tr>
                    </table>
                </td>
                <td width="2%">

                </td>
                <td width="56%" height="400px" style="vertical-align:top;margin:3px;padding-top:0px;padding-bottom:0px">
                    <div class="list-section-heading">
                        <a class="skill-edit-toggle" title="Toggle Skill Display">Skills</a>
                    </div>
                    <div style="height:390px;overflow:scroll">
                        <table class="stats-table" style="overflow:auto">
                            <tr style="text-align:left;margin:2px">
                                <th></th>
                                <th>{{localize "torgeternity.sheetLabels.attribute"}}</th>
                                <th>{{localize "torgeternity.sheetLabels.adds"}}</th>
                                <th>{{localize "torgeternity.sheetLabels.total"}}</th>
                            </tr>


                            {{#if data.editstate}}
                                {{#each data.skills as |content name|}}
                                    <tr>
                                        <td>
                                            <a class="skill-roll" data-testType="skill" data-baseAttribute="{{content.baseAttribute}}" data-adds="{{content.adds}}" data-value="{{content.value}}" data-unskilledUse="{{content.unskilledUse}}" data-name="{{localize (concatSkillName name)}}">{{localize (concatSkillName name)}}</a>
                                        </td>
                                        <td>
                                            {{localize (concatAttributeName content.baseAttribute)}}
                                        </td>
                                        <td>
                                            <input name="data.skills.{{name}}.adds" type="number" class="small-input" value="{{content.adds}}" data-dtype="Number" style="width:30px;text-align:center" />
                                        </td>
                                        <td style="text-align:center">
                                            {{content.value}}
                                        </td>
                                    </tr>
                                {{/each}}
                            {{else}}
                                {{#each data.skills as |content name|}}
                                    {{#if content.adds}}                                                                            
                                    <tr>
                                        <td>
                                            <a class="skill-roll" data-testType="skill" data-baseAttribute="{{content.baseAttribute}}" data-adds="{{content.adds}}" data-value="{{content.value}}" data-unskilledUse="{{content.unskilledUse}}" data-name="{{localize (concatSkillName name)}}">{{localize (concatSkillName name)}}</a>
                                        </td>
                                        <td class="skill-text">
                                            {{localize (concatAttributeName content.baseAttribute)}}
                                        </td>
                                        <td class="skill-text">
                                            <input name="data.skills.{{name}}.adds" type="number" class="small-input" value="{{content.adds}}" data-dtype="Number" style="width:30px;text-align:center" />
                                        </td>
                                        <td class="skill-text" style="text-align:center">
                                            {{content.value}}
                                        </td>
                                    </tr>
                                    {{/if}}
                                {{/each}}
                            {{/if}}

                        </table>
                    </div>
                </td>
                <td width="2%">

                </td>
                <td width="20%" style="vertical-align:top;margin:3px">
                    <div class="list-section-heading">
                        <a class="activeDefense-roll" title="Roll for Active Defense">{{localize
                            "torgeternity.sheetLabels.defenses"}}</a>
                    </div>
                    <table class="stats-table">
                        <tr>
                            <td>
                                <span class="main-label">{{localize "torgeternity.skills.dodge"}}</span>
                            </td>
                            <td>
                                <span class="main-label">{{data.dodgeDefense}}</span>
                            </td>
                        </tr>
                        <tr>
                            <td>
                                <span class="main-label">{{localize "torgeternity.sheetLabels.melee"}}</span>
                            </td>
                            <td>
                                <span class="main-label">{{data.meleeWeaponsDefense}}</span>
                            </td>
                        </tr>
                        <tr>
                            <td>
                                <span class="main-label">{{localize "torgeternity.sheetLabels.unarmed"}}</span>
                            </td>
                            <td>
                                <span class="main-label">{{data.unarmedCombatDefense}}</span>
                            </td>
                        </tr>
                        <tr>
                            <td>
                                <span class="main-label">{{localize "torgeternity.skills.intimidation"}}</span>
                            </td>
                            <td>
                                <span class="main-label">{{data.intimidationDefense}}</span>
                            </td>
                        </tr>
                        <tr>
                            <td>
                                <span class="main-label">{{localize "torgeternity.skills.maneuver"}}</span>
                            </td>
                            <td>
                                <span class="main-label">{{data.maneuverDefense}}</span>
                            </td>
                        </tr>
                        <tr>
                            <td>
                                <span class="main-label">{{localize "torgeternity.skills.taunt"}}</span>
                            </td>
                            <td>
                                <span class="main-label">{{data.tauntDefense}}</span>
                            </td>
                        </tr>
                        <tr>
                            <td>
                                <span class="main-label">{{localize "torgeternity.skills.trick"}}</span>
                            </td>
                            <td>
                                <span class="main-label">{{data.trickDefense}}</span>
                            </td>
                        </tr>
                    </table>
                    <div class="list-section-heading">
                        {{localize "torgeternity.sheetLabels.xp"}}
                    </div>
                    <table class="stats-table" style="margin-bottom:0px">
                        <tr>
                            <td>
                                <span class="main-label">{{localize "torgeternity.sheetLabels.unspent"}}</span>
                            </td>
                            <td>
                                <input name="data.xp.unspent" type="number" value="{{data.xp.unspent}}" data=dtype="Number" class="small-input" />
                            </td>
                        </tr>
                        <tr>
                            <td>
                                <span class="main-label">{{localize "torgeternity.sheetLabels.earned"}}</span>
                            </td>
                            <td>
                                <input name="data.xp.earned" type="number" value="{{data.xp.earned}}" data=dtype="Number" class="small-input" style="width:30px" />
                            </td>
                        </tr>
                    </table>

                    <p class="item-data-label" style="padding:0px 0px 0px 0px">
                        {{localize "torgeternity.sheetLabels.clearance"}}
                    </p>
                    <div class="input-box" style="font-size:14px">
                        {{localize (concatClearanceLevel data.details.clearance)}}
                    </div>

                    <div style="height:15px">

                    </div>

                    <p class="item-data-label" style="padding:0px 0px 0px 0px">
                        {{localize "torgeternity.sheetLabels.race"}}
                    </p>
                    <select name="data.details.race" class="input-box" style="font-size:14px">
                        {{#select data.details.race}}
                        {{#each config.races as |name type|}}
                        <option value="{{type}}">{{localize name}}</option>
                        {{/each}}
                        {{/select}}
                    </select>

                </td>
            </table>

        </div>

        <div class="tab perks" style="height:400px;overflow:scroll" data-group="primary" data-tab="perks">
            <div class="list-section-heading">
                {{localize "torgeternity.sheetLabels.perks"}}
            </div>
            <ol class="item-list">
                {{#each perk as |item id|}}
                <li class="item" data-item-id="{{item._id}}">
                    <div class="item-row flexrow">
                        <img src="{{item.img}}" title="{{item.name}}" width="30" height="30" />
                        <a class="item-name">{{item.name}}</a>
                        <div class="item-controls">
                            <a class="item-control item-tochat" title="Send to Chat"><i class="fas fa-comment"></i></a>
                            <a class="item-control item-edit" title="Edit Item"><i class="fas fa-edit"></i></a>
                            <a class="item-control item-delete" title="Delete Item"><i class="fas fa-trash"></i></a>
                        </div>
                    </div>
                    <div class="item-detail">
                        <div class="item-dropdown-description">
                            {{{item.data.description}}}
                            <p></p>
                            <div class="item-dropdown-extras">
                                {{item.data.category}}
                            </div>
                            <div class="item-dropdown-extras">
                                {{item.data.cosm}}
                            </div>
                            <div class="item-dropdown-extras">
                                {{item.data.timestaken}}x
                            </div>
                        </div>
                    </div>
                </li>
                {{/each}}
            </ol>
            <div class="list-section-heading">
                {{localize "torgeternity.sheetLabels.enhancements"}}
            </div>
            <ol class="item-list">
                {{#each enhancement as |item id|}}
                <li class="item" data-item-id="{{item._id}}">
                    <div class="item-row flexrow">
                        <img src="{{item.img}}" title="{{item.name}}" width="30" height="30" />
                        <a class="item-name">{{item.name}} (Enhances {{item.data.perk}})</a>
                        <div class="item-controls">
                            <a class="item-control item-tochat" title="Send to Chat"><i class="fas fa-comment"></i></a>
                            <a class="item-control item-edit" title="Edit Item"><i class="fas fa-edit"></i></a>
                            <a class="item-control item-delete" title="Delete Item"><i class="fas fa-trash"></i></a>
                        </div>
                    </div>
                    <div class="item-detail">
                        <div class="item-dropdown-description">
                            {{{item.data.description}}}
                        </div>
                    </div>
                </li>
                {{/each}}
            </ol>
        </div>

        <div class="tab gear" style="height:400px;overflow:scroll" data-group="primary" data-tab="gear">
            <div class="list-section-heading">
                {{localize "torgeternity.sheetLabels.weapons"}}
            </div>
            <ol class="item-list">
                {{#each meleeweapons as |item id|}}
                <li class="item" data-item-id="{{item._id}}">
                    <div class="item-row flexrow">
                        <img src="{{item.img}}" title="{{item.name}}" width="30" height="30" />
                        <a class="item-name">{{item.name}} (Melee Weapon)</a>
                        <div class="item-controls">
                            <a class="item-control item-attackRoll" title="Attack Roll"><i
                                    class="fas fa-dice-d20"></i></a>
                            <a class="item-control item-edit" title="Edit Item"><i class="fas fa-edit"></i></a>
                            <a class="item-control item-delete" title="Delete Item"><i class="fas fa-trash"></i></a>
                        </div>
                    </div>
                    <div class="item-detail">
                        <div class="item-dropdown-description">
                            {{{item.data.description}}}
                            <p></p>
                            <div class="item-dropdown-extras">
                                {{item.data.cosm}}
                            </div>
                            <div class="item-dropdown-extras">
                                TL {{item.data.techlevel}}
                            </div>
                            <div class="item-dropdown-extras">
                                {{item.data.price}} ({{item.data.value}})
                            </div>
                            <div class="item-dropdown-extras">
                                {{item.data.notes}}
                            </div>
                        </div>
                    </div>
                </li>
                {{/each}}
            </ol>
            <ol class="item-list">
                {{#each missileweapon as |item id|}}
                <li class="item" data-item-id="{{item._id}}">
                    <div class="item-row flexrow">
                        <img src="{{item.img}}" title="{{item.name}}" width="30" height="30" />
                        <a class="item-name">{{item.name}} (Missile Weapon)</a>
                        <div class="item-controls">
                            <a class="item-control item-attackRoll" title="Attack Roll"><i
                                    class="fas fa-dice-d20"></i></a>
                            <a class="item-control item-edit" title="Edit Item"><i class="fas fa-edit"></i></a>
                            <a class="item-control item-delete" title="Delete Item"><i class="fas fa-trash"></i></a>
                        </div>
                    </div>
                    <div class="item-detail">
                        <div class="item-dropdown-description">
                            {{{item.data.description}}}
                            <p></p>
                            <div class="item-dropdown-extras">
                                {{item.data.cosm}}
                            </div>
                            <div class="item-dropdown-extras">
                                TL {{item.data.techlevel}}
                            </div>
                            <div class="item-dropdown-extras">
                                {{item.data.price}} ({{item.data.value}})
                            </div>
                            <div class="item-dropdown-extras">
                                {{item.data.range}}
                            </div>
                            <div class="item-dropdown-extras">
                                Ammo: {{item.data.ammo}}
                            </div>
                            <div class="item-dropdown-extras">
                                {{item.data.notes}}
                            </div>
                        </div>
                    </div>
                </li>
                {{/each}}
            </ol>
            <ol class="item-list">
                {{#each firearm as |item id|}}
                <li class="item" data-item-id="{{item._id}}">
                    <div class="item-row flexrow">
                        <img src="{{item.img}}" title="{{item.name}}" width="30" height="30" />
                        <a class="item-name">{{item.name}} (Firearm)</a>
                        <div class="item-controls">
                            <a class="item-control item-attackRoll" title="Attack Roll"><i
                                    class="fas fa-dice-d20"></i></a>
                            <a class="item-control item-edit" title="Edit Item"><i class="fas fa-edit"></i></a>
                            <a class="item-control item-delete" title="Delete Item"><i class="fas fa-trash"></i></a>
                        </div>
                    </div>
                    <div class="item-detail">
                        <div class="item-dropdown-description">
                            {{{item.data.description}}}
                            <p></p>
                            <div class="item-dropdown-extras">
                                {{item.data.cosm}}
                            </div>
                            <div class="item-dropdown-extras">
                                TL {{item.data.techlevel}}
                            </div>
                            <div class="item-dropdown-extras">
                                {{item.data.price}} ({{item.data.value}})
                            </div>
                            <div class="item-dropdown-extras">
                                {{item.data.range}}
                            </div>
                            <div class="item-dropdown-extras">
                                Ammo: {{item.data.ammo}}
                            </div>
                            <div class="item-dropdown-extras">
                                {{item.data.notes}}
                            </div>
                        </div>
                    </div>
                </li>
                {{/each}}
            </ol>

            <ol class="item-list">
                {{#each heavyweapon as |item id|}}
                <li class="item" data-item-id="{{item._id}}">
                    <div class="item-row flexrow">
                        <img src="{{item.img}}" title="{{item.name}}" width="30" height="30" />
                        <a class="item-name">{{item.name}} (Heavy Weapon/Explosive)</a>
                        <div class="item-controls">
                            <a class="item-control item-attackRoll" title="Attack Roll"><i
                                    class="fas fa-dice-d20"></i></a>
                            <a class="item-control item-edit" title="Edit Item"><i class="fas fa-edit"></i></a>
                            <a class="item-control item-delete" title="Delete Item"><i class="fas fa-trash"></i></a>
                        </div>
                    </div>
                    <div class="item-detail">
                        <div class="item-dropdown-description">
                            {{{item.data.description}}}
                            <p></p>
                            <div class="item-dropdown-extras">
                                {{item.data.cosm}}
                            </div>
                            <div class="item-dropdown-extras">
                                TL {{item.data.techlevel}}
                            </div>
                            <div class="item-dropdown-extras">
                                {{item.data.price}} ({{item.data.value}})
                            </div>
                            <div class="item-dropdown-extras">
                                {{item.data.range}}
                            </div>
                            <div class="item-dropdown-extras">
                                Ammo: {{item.data.ammo}}
                            </div>
                            <div class="item-dropdown-extras">
                                {{item.data.notes}}
                            </div>
                        </div>
                    </div>
                </li>
