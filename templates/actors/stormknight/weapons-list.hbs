<h2>
	{{localize "torgeternity.sheetLabels.attacks"}}
</h2>
<h3>
	{{localize "torgeternity.sheetLabels.interactionAttacks"}}
</h3>
<div class="interaction-attack-list">
	<a class="interaction-attack" data-name="{{localize "torgeternity.skills.intimidation"}}" data-base-attribute={{data.data.attributes.spirit.value}} data-adds={{data.data.skills.intimidation.adds}} data-skill-value={{data.data.skills.intimidation.value}} data-attack-type="intimidation">
		{{localize "torgeternity.skills.intimidation"}}
	</a>
	&bull;
	<a class="interaction-attack" data-name="{{localize "torgeternity.skills.maneuver"}}" data-base-attribute={{data.data.attributes.dexterity.value}} data-adds={{data.data.skills.maneuver.adds}} data-skill-value={{data.data.skills.maneuver.value}} data-attack-type="maneuver">
		{{localize "torgeternity.skills.maneuver"}}
	</a>
	&bull;
	<a class="interaction-attack" data-name="{{localize "torgeternity.skills.taunt"}}" data-base-attribute={{data.data.attributes.charisma.value}} data-adds={{data.data.skills.taunt.adds}} data-skill-value={{data.data.skills.taunt.value}} data-attack-type="taunt">
		{{localize "torgeternity.skills.taunt"}}
	</a>
	&bull;
	<a class="interaction-attack" data-name="{{localize "torgeternity.skills.trick"}}" data-base-attribute={{data.data.attributes.mind.value}} data-adds={{data.data.skills.trick.adds}} data-skill-value={{data.data.skills.trick.value}} data-attack-type="trick">
		{{localize "torgeternity.skills.trick"}}
	</a>
</div>
{{#if meleeweapons}}
	<h3>{{localize "torgeternity.sheetLabels.meleeWeapons"}}</h3>
	<div class="item-list">
		{{#each meleeweapons as |item id|}}
			<li class="item" data-item-id="{{item._id}}">

				<div>
					<img src="{{item.img}}" title="{{item.name}}"/>

				</div>
				<span class="item-name">{{item.name}}
				</span>
				<div class="item-controls">
					<a class="item-control item-attackRoll" title="{{localize 'torgeternity.iconeTitles.attackRoll'}}">
						<i class="fas fa-dice-d20"></i>
					</a>
					<a class="item-control item-bonusRoll" title="{{localize 'torgeternity.iconeTitles.bonusRoll'}}">
						<i class="fas fa-dice-d6"></i>
					</a>
					<a class="item-control item-edit" title="{{localize 'torgeternity.iconeTitles.edit'}}">
						<i class="fas fa-edit"></i>
					</a>
					<a class="item-control item-delete" title="{{localize 'torgeternity.iconeTitles.delete'}}">
						<i class="fas fa-trash"></i>
					</a>
				</div>
			</li>
		{{/each}}
	</div>
{{/if}}
