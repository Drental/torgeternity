<div class="list-section-heading">
	{{localize "torgeternity.sheetLabels.weapons"}}
</div>
{{#if meleeweapons}}
<h2>{{localize "torgeternity.sheetLabels.meleeWeapons"}}</h2>
<ol class="item-list">
	{{#each meleeweapons as |item id|}}
	<li class="item block" data-item-id="{{item._id}}">
		<div class="item-row flexrow">
			<img src="{{item.img}}" title="{{item.name}}" />
			<a class="item-name">{{item.name}}
				({{localize 'torgeternity.sheetLabels.meleeWeapons'}})</a>
			<div class="item-controls">
				<a class="item-control item-attackRoll" title="{{localize 'torgeternity.iconeTitles.attackRoll'}}">
					<i class="fas fa-dice-d20"></i>
				</a>
				<a class="item-control item-edit" title="{{localize 'torgeternity.iconeTitles.edit'}}">
					<i class="fas fa-edit"></i>
				</a>
				<a class="item-control item-delete" title="{{localize 'torgeternity.iconeTitles.delete'}}">
					<i class="fas fa-trash"></i>
				</a>
			</div>
		</div>
		<div class="item-detail">
			<div class="item-dropdown-description">
				{{{item.data.description}}}
				<p></p>
				<div class="item-dropdown-extras">
					{{item.data.cosm}}
				</div>
				<div class="item-dropdown-extras">
					TL
					{{item.data.techlevel}}
				</div>
				<div class="item-dropdown-extras">
					{{item.data.price}}
					({{item.data.value}})
				</div>
				<div class="item-dropdown-extras">
					{{item.data.notes}}
				</div>
			</div>
		</div>
	</li>
	{{/each}}
</ol>
{{/if}}
